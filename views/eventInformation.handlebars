<div id="eventInformation">
  <nav class="navbar fixed-top navbar-expand-lg navbar-light bg-light" id="navMain">
    <div class="container">

      <div class="row">
        <div class="col-sm">
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".nav-collapse">
            <span class="navbar-toggler-icon"></span>
          </button>

          <div class="collapse navbar-collapse">
            <ul class="navbar-nav" id="navElements">
              <li class="nav-item active">
                <form action="/" method="get">
                  <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
                </form>
              </li>
              <li class="nav-item">
                <a class="nav-link" id="skillPageNavBtn" href="/skilledVolunteerSearch">Find Skilled Volunteer</a>
              </li>
              <li class="nav-item active">
                <a class="nav-link" href="/categoryHostSearch">Find Host by Category</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">Features</a>
              </li>
            </ul>
          </div>

        </div>
      </div>

      <div class="row justify-content-end">
        <div class="col-sm" >
          <div class = "header-btn" id="loginBtn">
            <button class="btn btn-success my-2 my-sm-0" type="submit">Login</button>
          </div>
        </div>
      </div>

    </div>
  </nav>
</div>

  <div class="container" id="eventInformationResult">
    {{#each table}}
    <h1>Event Information</h1><hr>
      <ul class="list-group" id="eventInfoRes">
        <li class="list-group-item">
          <dt> Event Name </dt>
          <p>{{this.eventname}} </p>
          <dt> Event Description </dt>
          <p> {{this.eventdescription}} </p>
          <dt> Event Date </dt>
          <p> {{this.eventdate}} </p>
          <dt> Event Meet Details </dt>
          <p> {{this.eventmeetdetails}} </p>
        </li>
    </ul>
  </div>
    <div class="container">
    <button class="btn btn-success">Volunteer!</button>
      {{/each}}
    </div>
