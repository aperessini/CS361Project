  <div class="container" id="eventInformationResult">
    <h1>Event Host Information</h1><hr>
      <ul class="list-group" id="eventInfoRes">
        <li class="list-group-item">
	  <dt> Host Organization </dt>
          <p>{{#each host}}
	  {{this.hostorg}}<br>
	  {{/each}}</p>
	  <dt> Email </dt>
	  <p>{{#each host}}
	  {{this.email}}<br>
	  {{/each}}</p>
	  <dt> Organization Category </dt>
	  <p>{{#each host}}
	  {{this.categoryname}}<br>
	  {{/each}}</p>
	  <dt> Current Volunteer Events </dt>
	  {{#if events}}
	  <p>{{#each events}}
          {{this.eventname}} <br>
          {{/each}}</p>
	  {{else}}
	  No events scheduled <br>
	  {{/if}}
        </li>
    </ul>
  </div>
